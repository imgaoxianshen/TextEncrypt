/**index.wxss**/
 .container{
  display:flex;
  flex-direction:column;
} 


.name{
  align-self: center;
  margin-top: 50rpx;
  font-size: 32rpx;
  font-weight: bold; 
}
 
 .circles{
  margin-top: 180rpx;
  align-self: center;
  height:200rpx;
}  

.circle{
  margin: 0rpx 0 0 0rpx;
  border-radius: 50%;
  width: 200rpx;
  height: 200rpx;
  background-color: #09bb07;
}
 .deep-circle{
  border-radius: 50%;
  margin: -194rpx 0 0 6rpx; 
  width: 192rpx;
  height: 192rpx;
   background-color: #fff; 
} 
.img_circle{
  display:block;
  border-radius: 50%;
  position: relative;
  top:16rpx;
  left: 16rpx;  
  width: 160rpx;
  height: 160rpx; 
}